{
  "name": "@jeesite/root",
  "version": "5.11.0",
  "type": "module",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "serve": "npm run dev",
    "dev": "cd web && pnpm run dev",
    "build": "turbo build --concurrency 20",
    "build:tomcat": "cd web && npm run build:tomcat",
    "build:preview": "cd web && npm run build:preview",
    "build:deploy": "turbo build:deploy && pnpm -r publish",
    "preview": "cd web && npm run preview",
    "preview:dist": "cd web && npm run preview:dist",
    "type:check": "turbo type:check --concurrency 20",
    "lint:eslint": "eslint --cache --max-warnings 0  \"./**/*.{ts,tsx,mjs,vue}\" --fix",
    "lint:prettier": "prettier --ignore-unknown --check --cache --write \"./**/*.{vue,tsx,less,scss}\"",
    "lint:stylelint": "stylelint \"./**/*.{vue,less,scss,css}\" --fix --custom-syntax postcss-html --cache --cache-location node_modules/.cache/stylelint/",
    "lint:all": "npm run bootstrap && npm run type:check && npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "uninstall": "turbo uninstall --concurrency 20 && rimraf pnpm-lock.yaml node_modules",
    "reinstall": "pnpm store prune && pnpm install --force && cd web && pnpm run dev --force",
    "update": "turbo update --concurrency 20 && ncu -u && npm run uninstall && npm run bootstrap"
  },
  "dependencies": {
    "@ant-design/colors": "7.2.0",
    "@ant-design/icons-vue": "7.0.1",
    "@jeesite/assets": "workspace:*",
    "@jeesite/build": "workspace:*",
    "@jeesite/core": "workspace:*",
    "@jeesite/dbm": "workspace:*",
    "@jeesite/test": "workspace:*",
    "@jeesite/types": "workspace:*",
    "@jeesite/web": "workspace:*",
    "ant-design-vue": "4.2.6",
    "axios": "1.7.9",
    "dayjs": "1.11.13",
    "lodash-es": "4.17.21",
    "vue": "3.5.13",
    "vue-eslint-parser": "9.4.3",
    "vue-router": "4.5.0",
    "vue-tsc": "2.2.4",
    "vue-types": "6.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.26.8",
    "@iconify/json": "2.2.310",
    "@iconify/utils": "2.3.0",
    "@stylistic/stylelint-plugin": "3.1.2",
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "@unocss/eslint-config": "66.0.0",
    "@unocss/preset-wind3": "66.0.0",
    "@vue/compiler-sfc": "3.5.13",
    "@vue/runtime-core": "3.5.13",
    "@vue/shared": "3.5.13",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "fs-extra": "11.3.0",
    "globals": "16.0.0",
    "less": "4.2.2",
    "npm-check-updates": "17.1.15",
    "pkg-types": "1.3.1",
    "postcss": "8.5.3",
    "postcss-html": "1.8.0",
    "postcss-less": "6.0.0",
    "prettier": "3.5.2",
    "prettier-plugin-packagejson": "2.5.8",
    "rimraf": "6.0.1",
    "stylelint": "16.14.1",
    "stylelint-config-recommended": "15.0.0",
    "stylelint-config-recommended-less": "3.0.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-less": "3.0.1",
    "stylelint-less": "3.0.1",
    "stylelint-prettier": "5.0.3",
    "ts-node": "10.9.2",
    "turbo": "2.4.2",
    "typescript": "5.7.3",
    "unocss": "66.0.0",
    "vite": "6.1.1"
  },
  "keywords": [
    "typescript",
    "jeesite",
    "antdv",
    "vite",
    "vue"
  ],
