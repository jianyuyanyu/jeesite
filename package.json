{
  "name": "@jeesite/root",
  "version": "5.10.0",
  "type": "module",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "serve": "npm run dev",
    "dev": "cd web && npm run dev",
    "build": "turbo build --concurrency 20",
    "build:tomcat": "cd web && npm run build:tomcat",
    "build:preview": "cd web && npm run build:preview",
    "build:deploy": "turbo build:deploy && pnpm -r publish",
    "preview": "cd web && npm run preview",
    "preview:dist": "cd web && npm run preview:dist",
    "type:check": "turbo type:check --concurrency 20",
    "lint:eslint": "eslint --cache --max-warnings 0  \"./**/*.{ts,tsx,vue}\" --fix",
    "lint:prettier": "prettier --write \"./**/*.{vue,tsx,less,scss}\"",
    "lint:stylelint": "stylelint \"./**/*.{vue,less,scss,css}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "lint:all": "npm run bootstrap && npm run type:check && npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "uninstall": "turbo uninstall --concurrency 20 && rimraf pnpm-lock.yaml node_modules",
    "reinstall": "pnpm store prune && pnpm install --force && cd web && pnpm run dev --force",
    "update": "turbo update --concurrency 20 && ncu -u -x eslint && npm run uninstall && npm run bootstrap",
    "upgrade": "node --loader=ts-node/esm ./upgrade/index.ts && npm run lint:all"
  },
  "dependencies": {
    "@ant-design/colors": "7.1.0",
    "@ant-design/icons-vue": "7.0.1",
    "@jeesite/assets": "workspace:*",
    "@jeesite/build": "workspace:*",
    "@jeesite/core": "workspace:*",
    "@jeesite/test": "workspace:*",
    "@jeesite/types": "workspace:*",
    "@jeesite/web": "workspace:*",
    "ant-design-vue": "4.2.5",
    "axios": "1.7.7",
    "dayjs": "1.11.13",
    "vue": "3.5.12",
    "vue-eslint-parser": "9.4.3",
    "vue-router": "4.4.5",
    "vue-types": "5.1.3",
    "vue-tsc": "2.1.6"
  },
  "devDependencies": {
    "@iconify/json": "2.2.260",
    "@iconify/utils": "2.1.33",
    "@typescript-eslint/eslint-plugin": "8.9.0",
    "@typescript-eslint/parser": "8.9.0",
    "@unocss/eslint-config": "0.63.4",
    "@vue/compiler-sfc": "3.5.12",
    "@vue/runtime-core": "3.5.12",
    "@vue/shared": "3.5.12",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.29.0",
    "fs-extra": "11.2.0",
    "less": "4.2.0",
    "npm-check-updates": "17.1.3",
    "pkg-types": "1.2.1",
    "postcss": "8.4.47",
    "postcss-html": "1.7.0",
    "postcss-less": "6.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "rimraf": "6.0.1",
    "stylelint": "16.10.0",
    "stylelint-config-recommended-less": "3.0.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-less": "3.0.1",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.2",
    "ts-node": "10.9.2",
    "turbo": "2.1.3",
    "typescript": "5.6.3",
    "unocss": "0.63.4",
    "vite": "5.4.9"
  },
  "keywords": [
    "typescript",
    "jeesite",
    "antdv",
    "vite",
    "vue"
  ],
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "homepage": "https://jeesite.com",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/thinkgem/jeesite-vue.git"
  },
  "bugs": {
    "url": "https://gitee.com/thinkgem/jeesite-vue/issues"
  },
  "author": {
    "name": "ThinkGem",
    "email": "thinkgem@163.com",
    "url": "https://gitee.com/thinkgem"
  },
  "engines": {
    "node": "18 || >=20"
  },
  "packageManager": "pnpm@9.12.1"
}
