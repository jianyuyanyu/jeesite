{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:deploy": {
      "dependsOn": ["^build:deploy"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type:check": {
      "cache": false,
      "persistent": true
    },
    "uninstall": {
      "cache": false,
      "persistent": true
    },
    "update": {
      "cache": false,
      "persistent": true
    }
  }
}
